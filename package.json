{
  "name": "client-v2",
  "version": "1.0.0",
  "description": "",
  "main": "bootstrapper.js",
  "scripts": {
    "start": "npm-run-all sass:build --parallel browsersync sass:watch start-app",
    "start-app": "electron .",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint .",
    "lint:watch": "nodemon -e js -x \"npm run lint\"",
    "sass:build": "node-sass --source-map true --include-path styles styles/main.scss .tmp/styles/main.css",
    "sass:watch": "nodemon -e scss -x \"npm run sass:build\"",
    "browsersync": "browser-sync start -f '**/*.js,**/*.html,.tmp/styles/*.css'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmisio/ob-client-v2.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rmisio/ob-client-v2/issues"
  },
  "homepage": "https://github.com/rmisio/ob-client-v2#readme",
  "devDependencies": {
    "babel-eslint": "^6.0.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.9.0",
    "browser-sync": "^2.13.0",
    "chai": "^3.5.0",
    "electron-prebuilt": "^1.2.3",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^2.2.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "backbone": "^1.3.3",
    "jquery": "^3.0.0"
  }
}
